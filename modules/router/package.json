{
  "name": "@ingress/router",
  "version": "0.0.1-alpha.0",
  "keywords": [
    "radix",
    "trie",
    "tree",
    "router",
    "path",
    "http route"
  ],
  "author": "Caleb Boyd <caleb.boyd@hotmail.com> (https://github.com/calebboyd)",
  "license": "MIT",
  "description": "A fast router middleware for ingress",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/router.js",
        "require": "./lib/cjs/router.js"
      },
      "default": "./lib/esm/router.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "docs": "typedoc src/router.ts --excludePrivate",
    "test": "tap \"src/**/*.test.ts\" --no-coverage --node-arg=--loader=ts-node/esm",
    "coverage": "npm run build && tap \"lib/cjs/**/*.test.js\" --coverage-report=html",
    "build": "rm -rf lib && tsc -b tsconfig.json tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json"
  },
  "dependencies": {
    "@ingress/http-status": "3.0.0-alpha.3",
    "app-builder": "^7.0.0",
    "reflect-annotations": "4.0.0-alpha.3",
    "router-tree-map": "^0.0.3",
    "secure-json-parse": "^2.4.0"
  }
}
