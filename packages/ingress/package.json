{
  "name": "ingress",
  "version": "5.0.0-alpha.7",
  "description": "Composable nodejs applications",
  "type": "module",
  "main": "./lib/cjs/main.js",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/main.js",
        "require": "./lib/cjs/main.js"
      },
      "default": "./lib/cjs/main.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf lib && tsc && npm run build:commonjs",
    "build:commonjs": "tsc --module commonjs --outDir lib/cjs && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json",
    "watch:typescript": "tsc -w",
    "watch:test": "jest --watchAll --detectOpenHandles",
    "watch": "npm run build && concurrently -k \"npm:watch:typescript\" \"npm:watch:test\" -c blue,green",
    "test": "jest"
  },
  "repository": {
    "directory": "modules/ingress",
    "type": "git",
    "url": "ssh://git@github.com/ingress/ingress.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "jest": {
    "resolver": "jest-ts-webcompat-resolver",
    "setupFilesAfterEnv": [
      "reflect-metadata"
    ],
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      ".*\\.js",
      ".*\\.util\\.spec"
    ]
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "server",
    "http",
    "application",
    "framework",
    "composable",
    "declarative",
    "middleware"
  ],
  "dependencies": {
    "@hapi/subtext": "~7.0.3",
    "@ingress/di": "^3.0.0-alpha.2",
    "@ingress/http-status": "^3.0.0-alpha.3",
    "app-builder": "^7.0.2",
    "destroy": "~1.0.4",
    "find-my-way": "^4.1.0",
    "on-finished": "~2.3.0",
    "reflect-annotations": "^4.0.0-alpha.3",
    "rxjs": "~6.6.7",
    "secure-json-parse": "^2.4.0",
    "uuid": "^8.3.2",
    "ws": "^7.4.5"
  },
  "peerDependencies": {
    "@types/ws": "^7.4.1"
  },
  "peerDependenciesMeta": {
    "@types/ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/destroy": "~1.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/on-finished": "~2.3.1",
    "@types/secure-json-parse": "^1.0.2",
    "@types/sinon": "^10.0.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "concurrently": "^6.0.2",
    "cross-fetch": "~3.1.4",
    "get-port": "~5.1.1",
    "jest": "^26.6.3",
    "jest-github-actions-reporter": "^1.0.3",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^10.0.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "gitHead": "6296e5f924b953795fd7572963184bc8b6479c71"
}
